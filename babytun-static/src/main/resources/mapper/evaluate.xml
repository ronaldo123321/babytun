<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itlaoqi.babytun.dao.EvaluateDAO">
    <select id="findByGoodsId" parameterType="long" resultType="com.itlaoqi.babytun.entity.Evaluate">
        select * from t_evaluate where goods_id = #{value} order by create_time desc
    </select>
</mapper>